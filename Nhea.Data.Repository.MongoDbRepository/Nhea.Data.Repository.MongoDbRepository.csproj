<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Serkan Yazıcıoğlu</Authors>
    <Company>Garaj</Company>
    <Description>Base repository classes for MongoDb.</Description>
    <PackageTags>repository, nhea, mongodb, nosql</PackageTags>
    <Version>1.1.0</Version>
    <PackageReleaseNotes>Added ability to change databasename and collection name on the fly.</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MongoDB.Driver" Version="2.7.0" />
    <PackageReference Include="Nhea" Version="1.5.1" />
  </ItemGroup>

</Project>
